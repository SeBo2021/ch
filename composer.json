{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-cached-adapter": "~1.0",
    "league/flysystem-sftp": "~1.0",
    "php": "^8.0",
    "algolia/algoliasearch-client-php": "^2.2",
    "barryvdh/laravel-debugbar": "^3.3",
    "elasticsearch/elasticsearch": "^7.13",
    "fideloper/proxy": "^4.2",
    "freyo/flysystem-qcloud-cos-v5": "^2.0",
    "fruitcake/laravel-cors": "^1.0",
    "geoip2/geoip2": "~2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "intervention/image": "^2.5",
    "jacobcyl/ali-oss-storage": "^2.1",
    "laravel/framework": "^8.0",
    "laravel/passport": "^10.1",
    "laravel/scout": "^9.2",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "matchish/laravel-scout-elasticsearch": "^5.0",
    "mews/captcha": "^3.1",
    "mews/purifier": "~3.0",
    "moell/laravel-layui-admin": "^1.0",
    "pbmedia/laravel-ffmpeg": "^7.5",
    "peinhu/aetherupload-laravel": "dev-master",
    "phpoffice/phpspreadsheet": "^1.12",
    "spatie/laravel-permission": "^3.11",
    "zgldh/qiniu-laravel-storage": "^0.10.3",
    "zhuzhichao/ip-location-zh": "^2.4",
    "ext-simplexml": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": ">1.9.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Plugin\\": "plugin/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/Http/Helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
